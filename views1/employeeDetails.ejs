<%- include('header') -%>

    <h1>Employees List!</h1>

    <table border="1" >
        <tr>
            <th>Employee Id</th>
           <th>Employee Name</th>
           <th>Employee Salary</th>
           <th>Department</th>
           <th>Joining Date</th>
           <th>Mobile Number</th>
           <th>Address</th>
        </tr>
    
        <% for(var i=0; i < data.length; i++) { %>
            <tr>
                <td><%= data[i].employeeId %></td>
                <td><%= data[i].name %></td>
                <td><%= data[i].salary %></td>
                <td><%= data[i].Department %></td>
                <td><%= data[i].joiningDate %></td>
                <td><%= data[i].mobileNumber %></td>
                <td><%= data[i].address %></td>
                
            </tr>
        <% } %>
        
 <%- include('footer') -%>