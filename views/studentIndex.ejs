    <script>
        console.log('Welcome To My Website.This Website Was Created By-Utkarsh Varshney');

let Wallpaper_indx=1;
let home=document.getElementById('wallpaper');
let next=document.getElementById('NextButton');
let previous=document.getElementById('PreviousButton');
let student=document.getElementById('studentdrop');
let studentdropmenu=document.getElementById('studentdropmenu');
let faculty=document.getElementById('facultydrop');
let facultydropmenu=document.getElementById('facultydropmenu');

let flag1=0;   //Used for drop menu appear and disappear
let flag2=0;

home.classList.remove

setInterval(() => {
    if(Wallpaper_indx>4){
        Wallpaper_indx=0;
    }

    if(Wallpaper_indx==0){
        home.classList.remove(`wallpaper-5`);
    }
    else{
        home.classList.remove(`wallpaper-${Wallpaper_indx}`);
    }
    home.classList.add(`wallpaper-${Wallpaper_indx+1}`);
    Wallpaper_indx+=1;
}, 8000);



//Smooth Scrolling
Array.from(document.getElementsByClassName('links')).forEach(anchor => {
    anchor.addEventListener('click', function (e) {
        e.preventDefault();

        document.querySelector(this.getAttribute('href')).scrollIntoView({
            behavior: 'smooth'
        });
    });
});



// Make forward and backward button
next.addEventListener('click',()=>{
    
    if(Wallpaper_indx>4){
        Wallpaper_indx=0;
    }

    if(Wallpaper_indx==0){
        home.classList.remove(`wallpaper-5`);
    }
    else{
        home.classList.remove(`wallpaper-${Wallpaper_indx}`);
    }
    home.classList.add(`wallpaper-${Wallpaper_indx+1}`);
    Wallpaper_indx+=1;
})

previous.addEventListener('click',()=>{
    
    if(Wallpaper_indx<1){
        Wallpaper_indx=5;
    }
    home.classList.remove(`wallpaper-${Wallpaper_indx}`);
    if(Wallpaper_indx==1){
        home.classList.add(`wallpaper-5`);
    }
    else{
        home.classList.add(`wallpaper-${Wallpaper_indx-1}`);
    }
    Wallpaper_indx-=1;
})



//Make Drop Menu Appear and disappear
faculty.addEventListener('click',()=>{
    if(flag1===1){
        facultydropmenu.style.opacity=`0`;
        flag1=0; 
    }
    else if(flag1==0 && flag2==0){
        facultydropmenu.style.opacity=`0.86`;
        flag1=1;
    }
    else if(flag1==0 && flag2!=0){
        studentdropmenu.style.opacity=`0`;
        flag2=0;
        facultydropmenu.style.opacity=`0.86`;
        flag1=1;
    }
});

student.addEventListener('click',()=>{
    if(flag2===1){
        studentdropmenu.style.opacity=`0`;
        flag2=0;
    }
    else if(flag1==0 && flag2==0){
        studentdropmenu.style.opacity=`0.86`;
        flag2=1;
    }
    else if(flag1!=0 && flag2==0){
        facultydropmenu.style.opacity=`0`;
        flag1=0; 
        studentdropmenu.style.opacity=`0.86`;
        flag2=1;
    }
});

   
    </script>
