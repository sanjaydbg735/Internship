        home.classList.remove(`wallpaper-${Wallpaper_indx}`);
    }
    home.classList.add(`wallpaper-${Wallpaper_indx+1}`);
    Wallpaper_indx+=1;
})

previous.addEventListener('click',()=>{
    
    if(Wallpaper_indx<1){
        Wallpaper_indx=5;
    }
    home.classList.remove(`wallpaper-${Wallpaper_indx}`);
    if(Wallpaper_indx==1){
        home.classList.add(`wallpaper-5`);
    }
    else{
        home.classList.add(`wallpaper-${Wallpaper_indx-1}`);
    }
    Wallpaper_indx-=1;
})



